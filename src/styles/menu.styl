nav {
    background cMainText
    margin-bottom 20px
    height 35px

    ul {
        margin 0
        padding 0

        li {
            display block
            float left
            height 20px
            margin-right 20px

            a {
                font-size 16px
                line-height 35px
                color cBackMain
                text-decoration none

                &.active {
                    color cHighlight
                }
            }
        }
    }
}